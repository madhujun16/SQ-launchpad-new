# Supabase Configuration
# Get these values from your Supabase project dashboard
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# LocationIQ is used for geocoding through Supabase Edge Functions
# No external API keys needed for this project

# LocationIQ API Key (if using as fallback)
# Get your API key from: https://locationiq.com/
VITE_LOCATIONIQ_API_KEY=your_locationiq_api_key_here

# Application Configuration
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0

# Security Configuration
VITE_ENABLE_DEBUG_LOGS=false
VITE_ENABLE_AUDIT_LOGS=true
